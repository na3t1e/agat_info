        {{ seoService.getOpenGraphImage(seoService.findByPath(app.request.pathinfo).title, app.request.pathinfo) }}
        <meta property="og:title" content='{{ seoService.findByPath(app.request.pathinfo).title|default('Авиакомпания АГАТ') }}' />
        <meta property="og:type" content="website" />
        <meta property="og:description" content="{{ seoService.findByPath(app.request.pathinfo).description|default('Авиакомпания АГАТ') }}" />
        <meta property="og:url" content="{{ app.request.uri }}" />
        <meta property="og:image" content="{{app.request.getSchemeAndHttpHost()}}/opengraph/og-{{ seoService.translatePathToKebabCase(app.request.pathinfo) }}.jpg" />
        <meta property="og:image" content=`{{ seoService.getOpenGraphImage(seoService.findByPath(app.request.pathinfo).title, app.request.pathinfo) }}` />

        <meta name="twitter:card" content="summary_large_image">
        <meta property="twitter:url" content="{{ app.request.uri }}">
        <meta name="twitter:title" content='{{ seoService.findByPath(app.request.pathinfo).title|default('Авиакомпания АГАТ') }}'>
        <meta name="twitter:description" content="{{ seoService.findByPath(app.request.pathinfo).description|default('Авиакомпания АГАТ') }}">
