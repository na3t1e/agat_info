        <meta name="description" content="{{ seoService.findByPath(app.request.pathinfo).description|default('Авиакомпания АГАТ') }}">
        <meta name="keywords" content="{{ seoService.findByPath(app.request.pathinfo).keywords|default('Авиакомпания АГАТ') }}" />

        <script>
                let title = document.getElementsByTagName("title")[0].innerText;
                document.querySelector('meta[property="og:title"]').setAttribute("content", title);
                document.querySelector('meta[name="twitter:title"]').setAttribute("content", title);
        </script>